<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cukcuklite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sell.menu.unit.UnitSelectedActivity"></activity>
        <activity android:name=".sell.menu.update.UpdateFoodInMenuActivity" />
        <activity android:name=".sell.menu.insert.InsertFoodToMenuActivity" />
        <activity
            android:name=".sell.MainActivity"
            android:label="@string/title_activity_sell_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".orderFood.OderFoodActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>